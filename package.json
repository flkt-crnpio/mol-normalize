{
  "name": "mol-normalize",
  "version": "3.0.0",
  "author": "flkt.crnpio@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flkt-crnpio/mol-normalize"
  },
  "main": "mol/normalize.min.css",
  "devDependencies": {
    "@types/bun": "latest",
    "sass": "^1.97.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "bugs": {
    "url": "https://github.com/flkt-crnpio/mol-normalize/issues"
  },
  "description": "normalized file customizable by css vars",
  "files": [
    "mol",
    "README.md"
  ],
  "homepage": "https://flkt-crnpio.github.io/mol-normalize/",
  "keywords": [
    "mol-normalize",
    "normalize",
    "css",
    "scss",
    "sass",
    "bun",
    "flkt"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "bun run vars & bun run normalize & bun run serve",
    "serve": "bun --watch run docs/index.html",
    "normalize": "sass --watch scss/normalize.scss:docs/normalize.css --style expanded",
    "vars": "sass --watch scss/_vars.scss:docs/vars.css --style expanded",
    "build": "sass scss/normalize.scss:mol/normalize.min.css --style compressed"
  }
}
